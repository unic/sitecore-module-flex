<?xml version="1.0" encoding="utf-8"?>
<Configuration>
  <MuckRecyleAppPool>false</MuckRecyleAppPool>
  <WebsiteCodeName>flex-website</WebsiteCodeName>
  <WebFolderName>Web</WebFolderName>
  <GlobalWebPath>d:\web</GlobalWebPath>
  <KeepAppConfigIncludes>
    *_sitecore;
    *_base;
    *_hardening;
    *_$Environment;
    *_$Role;
    *_$Environment.$Role
  </KeepAppConfigIncludes>
  <BennyFileWatcher>
    *.cshtml;
  </BennyFileWatcher>
  <AppPoolRuntime>v4.0</AppPoolRuntime>
  <HostsFileComment>generated by Bob Open Build and his Can Do Crew</HostsFileComment>
  <BackupFolderName>Backup</BackupFolderName>
  <SerializationReferenceTemplate>
    <![CDATA[
          <configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
            <sitecore>
              <sc.variable name="serializationPath" set:value=""/>
              <settings>
                  <setting name="SerializationFolder" set:value="$(serializationPath)/appDefault"/>
              </settings>
            </sitecore>
          </configuration>
          ]]>
  </SerializationReferenceTemplate>
  <SerializationReferenceXPath>configuration/sitecore/sc.variable/@set:value</SerializationReferenceXPath>
  <SerializationPath>..\..\serialization</SerializationPath>
  <SerializationReferenceFilePath>App_Config\Include\Unic.SerializationReference.config</SerializationReferenceFilePath>
  <InitDatabasesPath>..\..\databases</InitDatabasesPath>
  <DatabaseServer>localhost</DatabaseServer>
  <DatabaseBackupShare>\\corp.unic.com\sys\backup\unic-dev-mssql2</DatabaseBackupShare>
  <ConnectionStringsFolder>App_Config\ConnectionStrings.config</ConnectionStringsFolder>
  <UnmanagedFiles>
    App_Config\ConnectionStrings.config;
    App_Config\Unmanaged\*
  </UnmanagedFiles>
  <IISBindings>
    <Binding IP="">http://flex.local</Binding>
  </IISBindings>
  <NuGetFeed>https://teamcity.unic.com/httpAuth/app/nuget/v1/FeedService.svc/</NuGetFeed>
  <LoftyVersion>1.1.0</LoftyVersion>
  <!-- Dodger version can be left empty. The newest version will then be used. !-->
  <DodgerVersion></DodgerVersion>
</Configuration>