@using Unic.Flex.Context
@using Unic.Flex.Presentation
@inherits Unic.Flex.Presentation.FlexView<Unic.Flex.Model.ViewModel.Steps.StepViewModel, Unic.Flex.Model.DomainModel.Steps.MultiStep>

<p>NAVIGATION PANEL</p>

@for (int i = 0; i < Model.Sections.Count; i++)
{
    @Html.FlexComponent(m => m.Sections[i])
}


<a href="@FlexContext.Current.PreviousStepUrl">Back</a>
<a href="@FlexContext.Current.NextStepUrl">Skip</a>

<input type="submit" value="submit" />