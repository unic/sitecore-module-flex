@using System.Web.Mvc.Html
@model Unic.Flex.Implementation.Fields.InputFields.EmailFieldViewModel

<li class="@Model.CssClass">
    @{
        Model.Attributes.Add("type", "email");
    }

    @Html.LabelFor(m => m.Value, Model.Label)
    @Html.TextBoxFor(m => m.Value, Model.Attributes)
    @Html.ValidationMessageFor(model => model.Value)
</li>