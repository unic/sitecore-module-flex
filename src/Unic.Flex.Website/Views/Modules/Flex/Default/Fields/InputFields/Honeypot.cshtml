@using System.Web.Mvc.Html
@using Unic.Flex.Presentation
@model Unic.Flex.Model.ViewModel.Fields.InputFields.HoneypotFieldViewModel

<li class="flex_listitem @Model.CssClass">
    @Html.FlexLabel(m => m.Value, Model.Label)
    @Html.TextBoxFor(m => m.Value, Html.GetAttributes(Model))
    @Html.FlexValidator(model => model.Value)
</li>