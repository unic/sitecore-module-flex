@using System.Web.Mvc.Html
@using Unic.Flex.Presentation
@model Unic.Flex.Implementation.Fields.InputFields.SinglelineTextFieldViewModel

<li class="fm_listitem @Model.CssClass">
    @Html.FlexLabel(m => m.Value, Model.Label)
    @Html.TextBoxFor(m => m.Value, Html.GetAttributes(Model))
    @Html.ValidationMessageFor(model => model.Value)
</li>