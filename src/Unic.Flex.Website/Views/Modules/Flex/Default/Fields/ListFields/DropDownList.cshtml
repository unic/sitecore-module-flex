@using System.Web.Mvc.Html
@model Unic.Flex.Implementation.Fields.ListFields.DropDownListFieldViewModel

<li class="@Model.CssClass">
    @Html.LabelFor(m => m.Value, Model.Label)
    @Html.DropDownListFor(m => m.Value, Model.Items, Model.Attributes)
    @Html.ValidationMessageFor(model => model.Value)
</li>